{
  "name": "portfolio-v3",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "node-sass": "^4.7.2",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "gsap": "^1.20.4",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "npm-run-all": "^4.0.1",
    "pixi-filters": "^2.6.0",
    "pixi-particles": "^3.0.0",
    "pixi.js": "^4.7.3",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-helmet": "^5.0.0",
    "react-localization": "git+https://git@github.com/cidicles/react-localization.git",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.0.0-beta.6",
    "react-tooltip": "^3.5.1",
    "react-transition-group": "1.x",
    "redux": "^3.6.0",
    "redux-persist": "^5.4.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  }
}
